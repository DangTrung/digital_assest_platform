<div class="container mt-5">
  <div class="row justify-content-center">
    <div class="col-auto">
      <h1 class="text-center">Admin Dashboard</h1>
    </div>
  </div>

  <div class="row mt-5">
    <div class="col-12">
      <div class="border rounded p-4 shadow-sm">
        <h2 class="text-left">Creator Total Earning</h2>
        <%= column_chart @creator_total_eanring, xtitle: "Creator", ytitle: "Total Earnings" %>
      </div>
    </div>
  </div>

  <div class="row mt-5 mb-5">
    <div class="col-12">
      <div class="border rounded p-4 shadow-sm">
        <h2 class="text-left">Customer Total Earning</h2>
        <%= column_chart @customer_total_eanring, xtitle: "Customer", ytitle: "Total Earnings" %>
      </div>
    </div>
  </div>
</div>
